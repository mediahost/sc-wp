{
	"name": "majkl578/nette-identity-doctrine",
	"type": "nette-addon",
	"keywords": ["nette", "doctrine", "orm", "security", "identity", "authentication"],
	"authors": [
		{
			"name": "Michael Moravec"
		}
	],
	"description": "Integration of entities implementing IIdentity in Nette 2",
	"license": "MIT",
	"require": {
		"php": ">=5.3.2",
		"nette/di": "~2.2",
		"nette/http": "~2.2",
		"nette/security": "~2.2",
		"doctrine/orm": "~2.3"
	},
	"require-dev": {
		"nette/bootstrap": "~2.2",
		"kdyby/doctrine": "~2.0",
		"tracy/tracy": "~2.2",
		"phpunit/phpunit": "~4.8"
	},
	"suggests": {
		"nette/bootstrap": "To use IdentityExtension::register()."
	},
	"autoload": {
		"psr-4": {
			"Majkl578\\NetteAddons\\Doctrine2Identity\\": "lib"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Majkl578\\NetteAddons\\Doctrine2Identity\\Tests\\": "tests"
		}
	}
}
